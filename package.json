{
  "name": "imtiaz-trading-platform",
  "version": "1.0.0",
  "description": "Multi-branch trading platform with advanced features",
  "author": "Imtiaz Trading",
  "license": "MIT",
  "type": "module",
  "main": "electron-main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview",
    "start": "serve dist -l $PORT",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "electron:dev": "NODE_ENV=development electron .",
    "electron:build": "npm run build:prod && electron-builder",
    "electron:build:win": "npm run build:prod && electron-builder --win",
    "electron:build:mac": "npm run build:prod && electron-builder --mac",
    "electron:build:linux": "npm run build:prod && electron-builder --linux",
    "electron:build:all": "npm run build:prod && electron-builder -mwl",
    "android:sync": "npm run build:prod && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "npm run android:sync && cd android && ./gradlew assembleRelease",
    "android:bundle": "npm run android:sync && cd android && ./gradlew bundleRelease",
    "ios:sync": "npm run build:prod && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "cap:sync": "npm run build:prod && npx cap sync",
    "build:all": "npm run build:prod && npm run android:sync && npm run ios:sync"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@sentry/react": "^10.29.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "axios": "^1.13.2",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.263.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^14.2.1",
    "terser": "^5.36.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8",
    "vitest": "^4.0.15"
  }
}
